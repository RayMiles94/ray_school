<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="ray_school_partent_tree" model="ir.ui.view">
            <field name="name">ray.school.partent.tree</field>
            <field name="model">ray.school.partent</field>
            <field name="arch" type="xml">
                <tree string="Partent">
                    <field name="name"/>
                    <field name="email"/>
                    <field name="phone"/>
                    <field name="mobile"/>
                    <field name="join_date"/>
                    <field name="Status"/>
                </tree>
            </field>
        </record>

        <record id="ray_school_partent_form" model="ir.ui.view">
            <field name="name">ray.school.partent.form</field>
            <field name="model">ray.school.partent</field>
            <field name="arch" type="xml">
                <form string="Partent">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="email"/>
                                <field name="phone"/>
                            </group>
                            <group>
                                <field name="mobile"/>
                                <field name="join_date"/>
                                <field name="Status"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="ray_school_partent_action" model="ir.actions.act_window">
            <field name="name">Partent</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">ray.school.partent</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>